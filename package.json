{
  "name": "my-app",
  "version": "0.1.7",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-scripts": "4.0.1",
    "web-vitals": "1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "release-it"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@j-ulrich/release-it-regex-bumper": "1.2.0",
    "release-it": "14.2.2"
  },
  "release-it": {
    "github": {
      "release": true,
      "tokenRef": "RELEASE_IT_GITHUB_TOKEN"
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@j-ulrich/release-it-regex-bumper": {
        "out": [
          {
            "file": "settings-*.json"
          },
          {
            "file": "package.js"
          },
          {
            "file": "src/App.js"
          }
        ]
      }
    }
  }
}
